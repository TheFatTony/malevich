{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "malevich-landing": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "target/malevich-landing",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "src/assets/icon-line/css/simple-line-icons.css",
              "src/assets/icon-line-pro/style.css",
              "src/assets/icon-hs/style.css",
              "src/assets/hamburgers/hamburgers.min.css",
              "src/assets/chosen/chosen.css",
              "src/assets/slick-carousel/slick/slick.css",
              "src/assets/master-slider/source/assets/css/masterslider.main.css",
              "src/assets/malevich/css/styles.op-real-estate.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery-migrate/dist/jquery-migrate.js",
              "node_modules/jquery-countdown/dist/jquery.countdown.js",
              "src/assets/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/appear.js",
              "src/assets/slick-carousel/slick/slick.js",
              "src/assets/master-slider/source/assets/js/masterslider.min.js",
              "src/assets/js/hs.core.js",
              "src/assets/js/components/hs.header.js",
              "src/assets/js/helpers/hs.hamburgers.js",
              "src/assets/js/components/hs.scroll-nav.js",
              "src/assets/js/components/hs.rating.js",
              "src/assets/js/components/hs.carousel.js",
              "src/assets/js/components/hs.go-to.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "malevich-landing:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "malevich-landing:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "malevich-landing:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "malevich-landing-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "malevich-landing:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "malevich-landing:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "malevich-landing"
}
