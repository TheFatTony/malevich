    <changeSet author="toolstik" id="trader-address-fk">
        <addForeignKeyConstraint baseColumnNames="address_id" baseTableName="trader"
                                 constraintName="trader_address_fk"
                                 referencedColumnNames="id" referencedTableName="address"/>
    </changeSet>
</databaseChangeLog>