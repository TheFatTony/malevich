<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="thefattony" id="files-id-nn">
		<addNotNullConstraint columnDataType="bigint" columnName="id" tableName="files" />
	</changeSet>

	<changeSet author="thefattony" id="files-file_name-nn">
		<addNotNullConstraint columnDataType="varchar(4000)" columnName="file_name" tableName="files" />
	</changeSet>

	<changeSet author="thefattony" id="files-id-aa">
		<addAutoIncrement columnDataType="bigint" columnName="id" tableName="files" />
	</changeSet>

</databaseChangeLog>