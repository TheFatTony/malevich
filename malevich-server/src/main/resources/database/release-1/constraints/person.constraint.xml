<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet author="toolstik" id="person-gender-fk">
        <addForeignKeyConstraint baseColumnNames="gender_id" baseTableName="person"
                                 constraintName="person_gender_fk"
                                 referencedColumnNames="id" referencedTableName="gender"/>
    </changeSet>

    <changeSet author="toolstik" id="person-country-fk">
        <addForeignKeyConstraint baseColumnNames="country_id" baseTableName="person"
                                 constraintName="person_country_fk"
                                 referencedColumnNames="id" referencedTableName="country"/>
    </changeSet>
    
</databaseChangeLog>