<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet author="toolstik" id="trader-mobile-uc">
        <addUniqueConstraint columnNames="mobile" constraintName="trader_mobile_uc" tableName="trader"/>
    </changeSet>

    <changeSet author="thefattony" id="trader-person-fk">
        <addForeignKeyConstraint baseColumnNames="person_id" baseTableName="trader"
                                 constraintName="trader_person_fk"
                                 referencedColumnNames="id" referencedTableName="person"/>
    </changeSet>

    <changeSet author="toolstik" id="trader-gender-fk">
        <addForeignKeyConstraint baseColumnNames="gender_id" baseTableName="trader"
                                 constraintName="trader_gender_fk"
                                 referencedColumnNames="id" referencedTableName="gender"/>
    </changeSet>

    <changeSet author="toolstik" id="trader-country-fk">
        <addForeignKeyConstraint baseColumnNames="country_id" baseTableName="trader"
                                 constraintName="trader_country_fk"
                                 referencedColumnNames="id" referencedTableName="country"/>
    </changeSet>


</databaseChangeLog>