<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet author="toolstik" id="wish_list-participant_id-index">
        <createIndex indexName="wish_list_participant_id_idx" tableName="wish_list">
            <column name="participant_id"/>
        </createIndex>
    </changeSet>

    <changeSet author="toolstik" id="wish_list-participant_id-artwork_stock_id-index">
        <createIndex indexName="wish_list_participant_id_artwork_stock_id_idx" tableName="wish_list" unique="true">
            <column name="participant_id"/>
            <column name="artwork_stock_id"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>