<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet author="harshilmelegant" id="gallery_document-table">
        <createTable tableName="gallery_document">
            <column name="id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="gallery_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="document_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet author="harshilmelegant" id="gallery_document-pk">
        <addPrimaryKey columnNames="id" constraintName="gallery_document_pk" tableName="gallery_document"/>
    </changeSet>

    <changeSet author="harshilmelegant" id="gallery_document-id-aa" dbms="mysql">
        <addAutoIncrement columnDataType="bigint" columnName="id" tableName="gallery_document"/>
    </changeSet>

</databaseChangeLog>