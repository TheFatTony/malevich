<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
				   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

	<changeSet author="toolstik" id="organization_address-table">
		<createTable tableName="organization_address">
			<column name="organization_id" type="bigint">
				<constraints nullable="false"/>
			</column>
			<column name="address_id" type="bigint">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>

	<changeSet author="toolstik" id="organization_address-pk">
		<addPrimaryKey columnNames="organization_id, address_id" constraintName="organization_address_pk" tableName="organization_address"/>
	</changeSet>

</databaseChangeLog>