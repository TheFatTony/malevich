<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
				   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

	<changeSet author="toolstik" id="participant_type_document_type-table">
		<createTable tableName="participant_type_document_type">
			<column name="participant_type_id" type="char(3)">
				<constraints nullable="false"/>
			</column>
			<column name="document_type_id" type="char(10)">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>

	<changeSet author="toolstik" id="participant_type_document_type-pk">
		<addPrimaryKey columnNames="participant_type_id, document_type_id" constraintName="participant_type_document_type_pk" tableName="participant_type_document_type"/>
	</changeSet>

</databaseChangeLog>